{
  "name": "@nutriot/morphagene-options",
  "version": "0.1.1",
  "description": "Helper for editing Morphagene `options.txt` files",
  "exports": {
    ".": "./dist/index.js",
    "./fs": "./dist/fs.js"
  },
  "type": "module",
  "types": "./dist",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nutriot/morphagene-options.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint:deps": "knip --production",
    "lint:json": "eslint ./*.json --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "prepack": "npm run build",
    "prepare": "husky install",
    "test": "uvu --require tsm tests --ignore shared.ts"
  },
  "keywords": [
    "morphagene",
    "make noise",
    "modular",
    "modular synthesizers"
  ],
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "18",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.3",
    "knip": "^2.40.0",
    "lint-staged": "^15.0.2",
    "npm-run-all2": "^6.1.1",
    "tsm": "^2.3.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.(json|ts)": "eslint --cache --fix"
  },
  "packageManager": "pnpm@8.15.6+sha256.01c01eeb990e379b31ef19c03e9d06a14afa5250b82e81303f88721c99ff2e6f"
}
