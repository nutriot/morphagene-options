{
  "name": "@nutriot/morphagene-options",
  "version": "0.0.0",
  "description": "Helper for editing Morphagene `options.txt` files",
  "exports": "./dist/index.mjs",
  "types": "./dist",
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint:deps": "knip --production",
    "lint:json": "eslint ./*.json --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "prepack": "npm run build",
    "prepare": "husky install",
    "test": "uvu --require tsm tests --ignore shared.ts"
  },
  "keywords": [
		"morphagene",
		"make noise",
		"modular"
	],
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.3",
    "knip": "^2.40.0",
    "lint-staged": "^15.0.2",
    "npm-run-all2": "^6.1.1",
    "tsm": "^2.3.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.(json|ts)": "eslint --cache --fix"
  }
}
