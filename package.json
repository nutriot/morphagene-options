{
  "name": "@nutriot/morphagene-options",
  "version": "0.3.3",
  "description": "Helper for editing Morphagene `options.txt` files",
  "exports": {
    ".": "./dist/index.js",
    "./fs": "./dist/fs.js"
  },
  "type": "module",
  "types": "./dist",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nutriot/morphagene-options.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint:code": "biome check --no-errors-on-unmatched --vcs-enabled=true --vcs-use-ignore-file=true",
    "lint:deps": "knip --production",
    "lint": "npm-run-all --parallel lint:*",
    "prepack": "npm run build",
    "prepare": "lefthook install",
    "publish:npm": "np --no-yarn",
    "publish:jsr": "deno publish",
    "test": "uvu --require tsm tests --ignore shared.ts"
  },
  "keywords": [
    "morphagene",
    "make noise",
    "modular",
    "modular synthesizers"
  ],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^18.19.41",
    "knip": "^5.26.0",
    "lefthook": "^1.12.4",
    "np": "^10.0.7",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "tsdown": "^0.14.2",
    "tsm": "^2.3.0",
    "tsup": "^8.2.1",
    "typescript": "^5.5.3",
    "uvu": "^0.5.6",
    "valibot": "^0.36.0",
    "zod": "^3.23.8"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
